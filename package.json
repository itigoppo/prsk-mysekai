{
  "name": "prsk-mysekai",
  "version": "0.1.0",
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@mui/material-nextjs": "^7.0.0",
    "clsx": "^2.1.1",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "fixpack": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.25.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "private": true,
  "scripts": {
    "dev": "next dev -p 16000 --turbopack",
    "dev:path": "pathpida --ignorePath .gitignore --watch",
    "fix": "run-p --continue-on-error fix:*",
    "fix:css": "run-s fix:css:prettier",
    "fix:css:prettier": "prettier './**/*.css' --write",
    "fix:js": "run-s fix:js:eslint fix:js:prettier",
    "fix:js:eslint": "next lint --fix",
    "fix:js:prettier": "prettier './**/*.{js,jsx,mjs,ts,tsx}' --write",
    "fix:json": "prettier './**/*.json' --write",
    "fix:md": "prettier './**/*.md' --write",
    "fix:pack": "fixpack",
    "generate:path": "pathpida --ignorepath .gitignore --output ./src/lib",
    "lint": "next lint",
    "start": "next start"
  }
}
